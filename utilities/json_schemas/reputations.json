{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "count",
    "next",
    "previous",
    "results"
  ],
  "properties": {
    "count": {
      "$id": "#/properties/count",
      "type": "integer"
    },
    "next": {
      "$id": "#/properties/next",
      "type": "null"
    },
    "previous": {
      "$id": "#/properties/previous",
      "type": "null"
    },
    "results": {
      "$id": "#/properties/results",
      "type": "array",
      "items": {
        "$id": "#/properties/results/items",
        "type": "object",
        "required": [
          "id",
          "account",
          "updated_at",
          "rating",
          "acceptance_rate",
          "achievements"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/results/items/properties/id",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "client": {
            "$id": "#/properties/results/items/properties/client",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "created_by": {
            "$id": "#/properties/results/items/properties/created_by",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "account": {
            "$id": "#/properties/results/items/properties/account",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "created_at": {
            "$id": "#/properties/results/items/properties/created_at",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "updated_at": {
            "$id": "#/properties/results/items/properties/updated_at",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "deleted_at": {
            "$id": "#/properties/results/items/properties/deleted_at",
            "type": "null"
          },
          "rating": {
            "$id": "#/properties/results/items/properties/rating",
            "type": ["string", "null"],
            "pattern": "^([0-9]\\d*).([0-9]\\d*)$"
          },
          "acceptance_rate": {
            "$id": "#/properties/results/items/properties/acceptance_rate",
            "type": ["string", "null"],
            "pattern": "^([0-9]\\d*).([0-9]\\d*)$"
          },
          "ontime_rate": {
            "$id": "#/properties/results/items/properties/ontime_rate",
            "type": ["string", "null"],
            "pattern": "^([0-9]\\d*).([0-9]\\d*)$"
          },
          "achievements": {
            "$id": "#/properties/results/items/properties/achievements",
            "type": ["array", "null"]
          }
        }
      }
    }
  }
}
