{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "count",
        "next",
        "previous",
        "results"
    ],
    "properties": {
        "count": {
            "$id": "#/properties/count",
            "type": "integer"
        },
        "next": {
            "$id": "#/properties/next",
            "type": "null"
        },
        "previous": {
            "$id": "#/properties/previous",
            "type": "null"
        },
        "results": {
            "$id": "#/properties/results",
            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/results/items",
                "type": "object",
                "required": [
                    "id",
                    "created_at",
                    "updated_at",
                    "balance",
                    "balance_currency",
                    "payout_method",
                    "employer",
                    "account"
                ],
                "properties": {
                    "id": {
                        "$id": "#/properties/results/items/properties/id",
                        "type": "string"
                    },
                    "created_at": {
                        "$id": "#/properties/results/items/properties/created_at",
                        "type": "string"
                    },
                    "updated_at": {
                        "$id": "#/properties/results/items/properties/updated_at",
                        "type": "string"
                    },
                    "balance": {
                        "$id": "#/properties/results/items/properties/balance",
                        "type": ["string","null"]
                    },
                    "balance_currency": {
                        "$id": "#/properties/results/items/properties/balance_currency",
                        "type": ["string","null"]
                    },
                    "payout_method": {
                        "$id": "#/properties/results/items/properties/payout_method",
                        "type": ["string","null"]
                    },
                    "employer": {
                        "$id": "#/properties/results/items/properties/employer",
                        "type": "string"
                    },
                    "account": {
                        "$id": "#/properties/results/items/properties/account",
                        "type": "string"
                    }
                },
                "additionalProperties": true
            }
        }
    },
    "additionalProperties": false
}
